<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhou.shop.mapper.UnitMapper">

    <select id="retrieveByUnitName" resultType="com.zhou.shop.entity.Unit">
        select * from shop.unit where unit_name like '%${unitName}%'
    </select>
</mapper>